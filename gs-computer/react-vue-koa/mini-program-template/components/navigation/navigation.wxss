/* 引入iconfont */
@import "../../styles/iconfont.wxss";

.navigation {
  position: relative;
  padding-left: 220rpx;
  padding-right: 220rpx;
  height: 44px;
  background-color: #fff;
  display: flex;
  align-items: center;
}
.navigation .left {
  position: absolute;
  left: 32rpx;
  height: 44px;
  display: flex;
  align-items: center;
}
.back .iconfont {
  font-size: 60rpx;
}
.home {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  overflow: hidden;
}
.home .picture {
  width: 100%;
  height: 100%;
}
.title {
  width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  text-align: center;
}