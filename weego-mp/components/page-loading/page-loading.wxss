@keyframes move-animation {
  0% {
      -webkit-transform: translate(0, 0);
      transform: translate(0, 0);
  }
  25% {
      -webkit-transform: translate(-64px, 0);
      transform: translate(-64px, 0);
  }
  75% {
      -webkit-transform: translate(32px, 0);
      transform: translate(32px, 0);
  }
  100% {
      -webkit-transform: translate(0, 0);
      transform: translate(0, 0);
  }
}

.page-loading-wrap {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 750rpx;
  background: #fff;
}
.page-loading-wrap .circle-loader {
  width: 64px;
  height: 64px;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -32rpx;
  margin-top: -32rpx;
  -webkit-transform-origin: 16px 16px;
  transform-origin: 16px 16px;
  -webkit-animation: rotate-animation 5s infinite;
  animation: rotate-animation 5s infinite;
  -webkit-animation-timing-function: linear;
  animation-timing-function: linear;
}
.page-loading-wrap .circle-loader .circle {
  -webkit-animation: move-animation 2.5s infinite;
  animation: move-animation 2.5s infinite;
  -webkit-animation-timing-function: linear;
  animation-timing-function: linear;
  position: absolute;
  left: 50%;
  top: 50%;
}
.page-loading-wrap .circle-loader .circle-line {
  width: 64px;
  height: 24px;
  position: absolute;
  top: 4px;
  left: 0;
  -webkit-transform-origin: 4px 4px;
  transform-origin: 4px 4px;
}
.page-loading-wrap .circle-loader .l1 {
  -webkit-transform: rotate(0deg);
  transform: rotate(0deg);
}
.page-loading-wrap .circle-loader .l2 {
  -webkit-transform: rotate(90deg);
  transform: rotate(90deg);
}
.page-loading-wrap .circle-loader .l3 {
  -webkit-transform: rotate(180deg);
  transform: rotate(180deg);
}
.page-loading-wrap .circle-loader .l4 {
  -webkit-transform: rotate(270deg);
  transform: rotate(270deg);
}
.page-loading-wrap .circle-loader .circle-line .c1 {
  width: 8px;
  height: 8px;
  top: 50%;
  left: 50%;
  margin-top: -4px;
  margin-left: -4px;
  border-radius: 4px;
  -webkit-animation-delay: -0.3s;
  animation-delay: -0.3s;
}
.page-loading-wrap .circle-loader .circle-line .c2 {
  width: 16px;
  height: 16px;
  top: 50%;
  left: 50%;
  margin-top: -8px;
  margin-left: -8px;
  border-radius: 8px;
  -webkit-animation-delay: -0.6s;
  animation-delay: -0.6s;
}
.page-loading-wrap .circle-loader .circle-line .c3 {
  width: 24px;
  height: 24px;
  top: 50%;
  left: 50%;
  margin-top: -12px;
  margin-left: -12px;
  border-radius: 12px;
  -webkit-animation-delay: -0.9s;
  animation-delay: -0.9s;
}
.page-loading-wrap .circle-loader .circle-blue {
  background-color: #1f4e5a;
}
.page-loading-wrap .circle-loader .circle-red {
  background-color: #ff5955;
}
.page-loading-wrap .circle-loader .circle-yellow {
  background-color: #ffb265;
}
.page-loading-wrap .circle-loader .circle-green {
  background-color: #00a691;
}